{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapping": {
    "^@KentNabiz/shared(.*)$": "<rootDir>/../../../packages/shared/src$1"
  },
  "setupFilesAfterEnv": ["<rootDir>/test/setup-e2e.ts"],
  "testTimeout": 30000,
  "forceExit": true,
  "detectOpenHandles": true
}
