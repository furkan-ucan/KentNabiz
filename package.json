{
  "name": "kentnabiz",
  "private": true,
  "scripts": {
    "dev": "pnpm exec turbo run dev",
    "dev:api": "cd apps/api && pnpm exec nest start --watch",
    "build": "pnpm exec turbo run build",
    "build:api": "pnpm exec tsc -p apps/api/tsconfig.build.json",
    "test": "pnpm exec turbo run test",
    "lint": "pnpm exec eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm exec eslint . --ext .ts,.tsx --fix",
    "type-check": "pnpm exec turbo run type-check",
    "format": "pnpm exec prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "check-services": "node scripts/check-services.js",
    "clean": "node -e \"const fs=require('fs');['.turbo','tsconfig.tsbuildinfo','apps/api/dist','apps/web/dist','apps/web/.next','apps/mobile/dist','packages/shared/dist','packages/ui/dist','packages/design-tokens/dist','packages/shared/tsconfig.tsbuildinfo','packages/ui/tsconfig.tsbuildinfo','packages/design-tokens/tsconfig.tsbuildinfo'].forEach(p=>{try{fs.rmSync(p,{recursive:true,force:true})}catch(e){}})\"",
    "reset": "pnpm run clean && node -e \"try{require('fs').rmSync('node_modules',{recursive:true,force:true})}catch(e){}\"",
    "fresh-start": "pnpm run reset && pnpm install --frozen-lockfile && pnpm build",
    "typeorm:api": "pnpm exec typeorm -d apps/api/dist/config/data-source.js",
    "migration:run": "pnpm typeorm:api migration:run",
    "migration:revert": "pnpm typeorm:api migration:revert",
    "migration:show": "pnpm typeorm:api migration:show",
    "schema:drop": "pnpm typeorm:api schema:drop",
    "migration:generate": "pnpm typeorm:api migration:generate apps/api/src/database/migrations/YourMigrationName",
    "seed:direct": "pnpm exec ts-node --project apps/api/tsconfig.json -r tsconfig-paths/register apps/api/src/database/seeds/index.ts",
    "db:reset": "pnpm run build:api && pnpm schema:drop && pnpm migration:run && pnpm seed:direct"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@nestjs/cli": "^11.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "dotenv": "^16.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.0",
    "multer": "1.4.5-lts.2",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.3.0",
    "turbo": "^1.13.4",
    "typeorm": "^0.3.21",
    "typescript": "^5.8.2",
    "typescript-eslint": "^7.3.1"
  },
  "dependencies": {
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@types/geojson": "^7946.0.16",
    "bcryptjs": "^2.4.3",
    "geojson": "^0.5.0",
    "minio": "^8.0.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "redis": "^4.7.0",
    "uuid": "^9.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "@chakra-ui/react": "3.19.2",
      "storybook": "8.6.14"
    }
  }
}
